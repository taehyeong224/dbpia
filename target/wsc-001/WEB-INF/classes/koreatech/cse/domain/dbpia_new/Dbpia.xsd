<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Dbpia" type="DbpiaType"/>
  <xs:complexType name="publisherType">
    <xs:sequence>
      <xs:element type="xs:string" name="url"/>
      <xs:element type="xs:string" name="name"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="itemType">
    <xs:sequence>
      <xs:element type="xs:string" name="ctype"/>
      <xs:element type="xs:string" name="title"/>
      <xs:element type="authorsType" name="authors"/>
      <xs:element type="publisherType" name="publisher"/>
      <xs:element type="publicationType" name="publication"/>
      <xs:element type="issueType" name="issue"/>
      <xs:element type="xs:string" name="pages"/>
      <xs:element type="xs:string" name="free_yn"/>
      <xs:element type="xs:string" name="preview_yn"/>
      <xs:element type="xs:string" name="preview"/>
      <xs:element type="xs:string" name="coverimg"/>
      <xs:element type="xs:string" name="link_url"/>
      <xs:element type="xs:string" name="link_api"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Dbpia">
    <xs:sequence>
      <xs:element type="rootType" name="root"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="paramdataType">
    <xs:sequence>
      <xs:element type="keywordType" name="keyword"/>
      <xs:element type="xs:string" name="totalcount"/>
      <xs:element type="xs:string" name="pagecount"/>
      <xs:element type="xs:string" name="pagenumber"/>
      <xs:element type="xs:string" name="category"/>
      <xs:element type="xs:string" name="sort"/>
      <xs:element type="xs:string" name="order"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="authorType">
    <xs:sequence>
      <xs:element name="order">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="1"/>
            <xs:enumeration value="2"/>
            <xs:enumeration value="3"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="url">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="
http://www.dbpia.co.kr/Author/AuthorInfo/?arcId=&amp;ancId=430324
"/>
            <xs:enumeration value="
http://www.dbpia.co.kr/Author/AuthorInfo/?arcId=&amp;ancId=430207
"/>
            <xs:enumeration value="
http://www.dbpia.co.kr/Author/AuthorInfo/?arcId=&amp;ancId=430277
"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="name">
        <xs:complexType mixed="true">
          <xs:sequence>
            <xs:element type="spanType" name="span" minOccurs="0"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="publicationType">
    <xs:sequence>
      <xs:element type="xs:string" name="url"/>
      <xs:element type="xs:string" name="name"/>
      <xs:element type="xs:string" name="issn"/>
      <xs:element type="xs:string" name="isbn10"/>
      <xs:element type="xs:string" name="isbn13"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="authorsType">
    <xs:sequence>
      <xs:element type="authorType" name="author" maxOccurs="unbounded" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="nameType">
    <xs:sequence>
      <xs:element type="spanType" name="span" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="issueType">
    <xs:sequence>
      <xs:element type="xs:string" name="name"/>
      <xs:element type="xs:string" name="num"/>
      <xs:element type="xs:string" name="yymm"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="rootType">
    <xs:sequence>
      <xs:element type="paramdataType" name="paramdata"/>
      <xs:element type="resultType" name="result"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="itemsType">
    <xs:sequence>
      <xs:element type="itemType" name="item"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="keywordType">
    <xs:sequence>
      <xs:element type="xs:string" name="searchall"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="spanType">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute type="xs:string" name="class"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="resultType">
    <xs:sequence>
      <xs:element type="itemsType" name="items"/>
    </xs:sequence>
  </xs:complexType>
</xs:schema>